{
  "name": "adopt-me",
  "version": "1.0.0",
  "description": "An app for finding pet written in react",
  "main": "src/App.js",
  "scripts": {
    "test": "jest --silent",
    "test:update": "jest --silent -u",
    "test:watch": "jest --silent --watch",
    "test:coverage": "jest --silent --coverage",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "dev": "parcel src/index.html",
    "build": "react-scripts build",
    "build:server": "parcel build --public-url ./dist/ src/index.html",
    "start": "npm run build:server && babel-node server.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikasnishal/react_petfinder.git"
  },
  "author": "vikas kumar",
  "license": "Apache-2.0",
  "bugs": {},
  "homepage": "https://vikasnishal.github.io/react_petfinder/",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "dotenv": "^6.2.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.2",
    "react-scripts": "^2.1.1",
    "react-test-renderer": "^16.6.3"
  },
  "dependencies": {
    "@reach/router": "^1.1.1",
    "babel-cli": "^6.26.0",
    "emotion": "^9.2.12",
    "express": "^4.16.4",
    "petfinder-client": "^0.0.3",
    "react": "^16.6.3",
    "react-dom": "^16.4.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
